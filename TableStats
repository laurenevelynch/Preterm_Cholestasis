library(readxl)
library(dplyr)
library(tibble)
meta <- read_excel("D:/Preidis Lab/Clinical Project/Metadata_clinical - LG.xlsx")
meta$days140 = as.numeric(meta$days140)
meta_norm = meta %>% filter(Cholestatic %in% c("No"))
library(rstatix)
#table 1
meta %>% group_by(Cholestatic) %>% get_summary_stats(GA, type = "mean_sd")
meta %>% t_test(GA ~ Cholestatic, paired = F) %>% add_significance()
meta %>% group_by(Cholestatic) %>% get_summary_stats(BW, type = "mean_sd")
meta %>% t_test(BW ~ Cholestatic, paired = F) %>% add_significance()
meta %>% group_by(Cholestatic) %>% get_summary_stats(D_14AB, type = "mean_sd")
meta %>% t_test(D_14AB ~ Cholestatic, paired = F) %>% add_significance()
meta %>% freq_table(Cholestatic, Gender)
fisher.test(meta$Cholestatic, meta$Gender)
meta %>% freq_table(Cholestatic, SGA_B)
fisher.test(meta$Cholestatic, meta$SGA_B)
meta %>% freq_table(Cholestatic, C_SECTION)
fisher.test(meta$Cholestatic, meta$C_SECTION)
meta %>% freq_table(Cholestatic, MOM_DM)
fisher.test(meta$Cholestatic, meta$MOM_DM)
meta %>% freq_table(Cholestatic, REC_INDO)
fisher.test(meta$Cholestatic, meta$REC_INDO)
meta %>% freq_table(Cholestatic, REC_SURF)
fisher.test(meta$Cholestatic, meta$REC_SURF)
meta %>% group_by(Cholestatic) %>% get_summary_stats(apgars5, type = "mean_sd")
meta %>% t_test(apgars5 ~ Cholestatic, paired = F) %>% add_significance()
meta %>% freq_table(Cholestatic, `2_dster`)
fisher.test(meta$Cholestatic, meta$`2_dster`)
#table 2
meta %>% group_by(Cholestatic) %>% get_summary_stats(D_TPN, type = "mean_sd")
meta %>% t_test(D_TPN ~ Cholestatic, paired = F) %>% add_significance()
meta %>% group_by(Cholestatic) %>% get_summary_stats(`D_NPO > 12 h`)
meta %>% t_test(`D_NPO > 12 h` ~ Cholestatic) %>% add_significance()
meta %>% filter(Patient_ID != 125) %>% group_by(Cholestatic) %>% get_summary_stats(days140, type = "mean_sd")
meta %>% t_test(days140 ~ Cholestatic, paired = F) %>% add_significance()
meta %>% freq_table(Cholestatic, Late_sep)
fisher.test(meta$Cholestatic, meta$Late_sep)
meta %>% freq_table(Cholestatic, DEATH)
fisher.test(meta$Cholestatic, meta$DEATH)
meta$LEN_STAY = as.numeric(meta$LEN_STAY)
meta %>%group_by(Cholestatic) %>% get_summary_stats(LEN_STAY, type = "mean_sd")
meta %>% t_test(LEN_STAY ~ Cholestatic, paired = F) %>% add_significance()
meta$WEIGHT_36 = as.numeric(meta$WEIGHT_36)
meta %>%group_by(Cholestatic) %>% get_summary_stats(WEIGHT_36, type = "mean_sd")
meta %>% t_test(WEIGHT_36 ~ Cholestatic, paired = F) %>% add_significance()
meta$len_36 = as.numeric(meta$len_36)
meta %>%group_by(Cholestatic) %>% get_summary_stats(len_36, type = "mean_sd")
meta %>% t_test(len_36 ~ Cholestatic, paired = F) %>% add_significance()
meta$HC_36 = as.numeric(meta$HC_36)
meta %>%group_by(Cholestatic) %>% get_summary_stats(HC_36, type = "mean_sd")
meta %>% t_test(HC_36 ~ Cholestatic, paired = F) %>% add_significance()
meta$GROWTH_VEL = as.numeric(meta$GROWTH_VEL)
meta %>%group_by(Cholestatic) %>% get_summary_stats(GROWTH_VEL, type = "mean_sd")
meta %>% t_test(GROWTH_VEL ~ Cholestatic, paired = F) %>% add_significance()
meta$Len_v = as.numeric(meta$Len_v)
meta %>%group_by(Cholestatic) %>% get_summary_stats(Len_v, type = "mean_sd")
meta %>% t_test(Len_v ~ Cholestatic, paired = F) %>% add_significance()
meta$HC_VEL = as.numeric(meta$HC_VEL)
meta %>%group_by(Cholestatic) %>% get_summary_stats(HC_VEL, type = "mean_sd")
meta %>% t_test(HC_VEL ~ Cholestatic, paired = F) %>% add_significance()
meta %>% freq_table(Cholestatic, BPD)
fisher.test(meta$Cholestatic, meta$BPD)
meta %>% freq_table(Cholestatic, SIP)
fisher.test(meta$Cholestatic, meta$SIP)
